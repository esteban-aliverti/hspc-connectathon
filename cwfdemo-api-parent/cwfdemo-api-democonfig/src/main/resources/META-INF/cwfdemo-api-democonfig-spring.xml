<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:cwp="http://www.carewebframework.org/schema/plugin"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans-3.1.xsd 
		http://www.carewebframework.org/schema/plugin
		http://www.carewebframework.org/schema/plugin/plugin-extensions.xsd">

<!-- This profile is processed during web server startup -->

	<beans profile="root">
	
		<bean id="cwfDemoConfigBootstrapper" 
			class="org.hspconsortium.cwfdemo.api.democonfig.Bootstrapper">
			<constructor-arg ref="fhirService" />
			<constructor-arg value="${cwfdemoconfig.scenario.base}" />
		</bean>
		
	</beans>
	
</beans>
