<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation=
		"http://www.springframework.org/schema/beans 
		 http://www.springframework.org/schema/beans/spring-beans.xsd">

	<beans profile="root">
		<bean class="org.hspconsortium.cwfdemo.setup.DemoSetup" depends-on="cwfHibernateDataSource">
			<constructor-arg value="${org.carewebframework.hibernate.url}" />
			<constructor-arg value="${org.carewebframework.hibernate.username}" />
			<constructor-arg value="${org.carewebframework.hibernate.password}" />
			<constructor-arg value="classpath:org/hspconsortium/cwfdemo/setup/setup.sql" />
		</bean>
	</beans>
</beans>
